{
    "name": "classid/laravel-audit-trails",
    "type": "package",
    "description": "laravel audit trails",
    "requires": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/contracts": "^9.0 ||  ^10.0",
        "illuminate/support": "^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "CID\\AuditTrails\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers":[
                "CID\\AuditTrails\\AuditTrailServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
